{
    "$schema": "https://json.schemastore.org/prettierrc.json",
    "semi": true,
    "tabWidth": 2,
    "printWidth": 120,
    "useTabs": false,
    "singleQuote": true,
    "trailingComma": "all",
    "jsxBracketSameLine": false,
    "bracketSpacing": true,
    "arrowParens": "always",
    "endOfLine": "lf",
    "quoteProps": "as-needed"
}